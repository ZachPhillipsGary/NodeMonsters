<!doctype html>
<html>
  <head>
<script src="https://code.jquery.com/jquery-2.2.2.min.js"   integrity="sha256-36cp2Co+/62rEAAYHLmRCPIych47CvdM+uTBJwSzWjI="   crossorigin="anonymous"></script>
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
<style type="text/css">
        form { background: #000; padding: 3px; position: relative; bottom: 0; width: 100%; }
      form input { border: 0; padding: 10px; width: 90%; margin-right: .5%; }
      form button { width: 9%; background: rgb(130, 224, 255); border: none; padding: 10px; }
      #messages { list-style-type: none; margin: 0; padding: 0; }
      #messages li { padding: 5px 10px; }
      #messages li:nth-child(odd) { background: #eee; }
</style>
  </head>
  <body>
  <div class="container-fluid">
<div class="row">
<div class="col-lg-12 panel panel-default">{{ username }}</div>
</div>
    <div class="row row-centered">
      <div class="col-lg-8">
        <canvas id="Canvas" width="5000" height="5000" >
        Your browser does not support the HTML5 canvas tag.
        </canvas>
      </div>
      <div class="col-lg-4" >
      <div id="Chat"></div>
      </div>
    </div>
    <div class="row row-centered">
    <h2>{{ username }}'s Monsters</h2>
    <hr>
        <div class="col-xs-6 col-centered">Bob<br><div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:70%">
      <span class="sr-only">70% Complete</span>
    </div>
  </div></div>
        <div class="col-xs-6 col-centered">Jon<br><div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%">
      <span class="sr-only">40% Complete</span>
    </div>
  </div></div>
        <div class="col-xs-3 col-centered">Kai<br><div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100" style="width:30%">
      <span class="sr-only">30% Complete</span>
    </div>
  </div></div>
        <div class="col-xs-3 col-centered">Test<br><div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width:20%">
      <span class="sr-only">20% Complete</span>
    </div>
  </div></div>
        <div class="col-xs-3 col-centered">Test2<br><div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="11" aria-valuemin="0" aria-valuemax="100" style="width:11%">
      <span class="sr-only">11% Complete</span>
    </div>
  </div></div>
    </div>
  </div>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/player.js"></script>
  <script src="/js/client.js"></script>
<script>
var init = new client("Canvas","Chat","{{ username }}");
  </script>
</body>
</html>
